# OWASP Dependency Check Suppressions for BucikaGSR

<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Android SDK components -->
    <suppress>
        <notes><![CDATA[
        Android SDK components are managed by Google and considered safe.
        ]]></notes>
        <gav regex="true">com\.android\..*:.*:.*</gav>
        <cve>CVE-2023-*</cve>
    </suppress>
    
    <!-- Suppress false positives for AndroidX libraries -->
    <suppress>
        <notes><![CDATA[
        AndroidX libraries are official Google libraries with regular security updates.
        ]]></notes>
        <gav regex="true">androidx\..*:.*:.*</gav>
        <cve>CVE-2023-*</cve>
    </suppress>
    
    <!-- Suppress false positives for Kotlin stdlib -->
    <suppress>
        <notes><![CDATA[
        Kotlin standard library is maintained by JetBrains and regularly updated.
        ]]></notes>
        <gav regex="true">org\.jetbrains\.kotlin:kotlin-stdlib.*:.*</gav>
        <cve>CVE-2023-*</cve>
    </suppress>
    
    <!-- Example: Suppress specific CVE if determined to be false positive -->
